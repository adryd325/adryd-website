---
// import Default from "../layouts/Default.astro";
import Friends from "../components/Friends.astro";
import Default from "../layouts/Default.astro";
import Footer from "../components/Footer.astro";
import NekoJS from "../components/NekoJS.astro";
import Construction from "../components/Construction.astro";
---

<Default title="home">
  <style>
    ul,
    ol {
      padding-left: 0;
      list-style-type: none;
    }
    .inline-flex {
      display: inline-flex;
    }
    h1 {
      margin: 3rem 0 1rem;
      padding: 0;
      font-weight: 900;
      font-size: 5rem;
    }
    h2 {
      font-weight: 900;
      font-size: 2.5rem;
      margin: 1.5rem 0 0;
    }
    .subtitle {
      font-weight: 500;
      margin: 0;
    }
  </style>
  <main>
   Error {"{{ err.status_code | rp.status_code }}"}
  </main>
  <Footer />
</Default>
